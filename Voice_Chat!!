<script type="text/javascript">
function createConference() {
    $.ajax({
        'url': '/api/v1/conference/',
        'type': 'POST',
        'success': function(data) {
            window.location.href = "/" + data['conference_name'] + "/";
        }
    });
}
</script>
<script src="/static/js/prism.js"></script>
